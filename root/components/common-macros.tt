    IF text;
      '<a href="' _ link _ '">' _ html_escape(text) _ '</a>';
      '</span>' IF url.edits_pending AND action == 'show';
    ELSE;
      text = display_url(url);
      '<a href="' _ url.url _ '">' _ text _ '</a >';
      '</span>' IF url.edits_pending AND action == 'show';
      ' [<a href="' _ link _ '">' _ l('info') _ '</a>]';
    END;
END -%]

