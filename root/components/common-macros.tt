[%- END -%]

[%- MACRO script_manifest(manifest) BLOCK -%]
  [%- IF server_details.developement_server -%]
    [%- USE date -%]
    [%- FOR file IN c.model('FileCache').manifest_files(manifest, 'js'); -%]
        <script src="/static/[%- file -%]?t=[% date.now %]"></script>
    [%- END -%]
  [%- ELSE -%]
    <script src="/static/[% c.model('FileCache').manifest_signature(manifest, 'js') %].js"></script>
  [%- END -%]
[%- END -%]

[%- MACRO css_manifest(manifest) BLOCK -%]
  [%- IF server_details.developement_server -%]
    [%- FOR file IN c.model('FileCache').manifest_files(manifest, 'css'); -%]
        <link rel="stylesheet" type="text/css" href="/static/[%- file -%]" />
    [%- END -%]
  [%- ELSE -%]
    <link rel="stylesheet" type="text/css" href="/static/styles/[% c.model('FileCache').manifest_signature(manifest, 'css') %].css" />
  [%- END -%]
[%- END -%]

[%- MACRO format_length(n) BLOCK; n | format_length; END %]
