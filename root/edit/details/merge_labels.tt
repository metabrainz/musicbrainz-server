<table class="details">
  <tr>
    <th>[%- l('Old labels:') %]</th>
    <td>
      <ul>
        [% FOR label=edit.display_data.old %]
        <li>[% link_label(label) %]</li>
        [% END %]
      </ul>
    </td>
  </tr>

  <tr>
    <th>[%- l('New label:') %]</th>
    <td>[% link_label(edit.display_data.new) %]</td>
  </tr>
</table>
