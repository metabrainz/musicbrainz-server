<table class="details remove-series-alias">
  <tr>
    <th>[%- add_colon(lp('Series', 'singular')) -%]</th>
    <td>[%- link_entity(edit.display_data.series) -%]</td>
  </tr>
  <tr>
    <th>[%- l('Alias:') -%]</th>
    <td>[%- edit.display_data.alias | html -%]
        [%- IF entity_exists(edit.display_data.series) -%]
          ([%- link_entity(edit.display_data.series, 'aliases', l('view all aliases')) -%])
        [%- END -%]
    </td>
  </tr>
  [%- IF edit.display_data.sort_name -%]
  <tr>
    <th>[%- l('Sort name:') -%]</th>
    <td>[%- (edit.display_data.sort_name) | html -%]</td>
  </tr>
  [%- END -%]
  <tr>
    <th>[%- l('Locale:') -%]</th>
    <td>[%- edit.display_data.locale | locale -%]</td>
  </tr>
  <tr>
    <th>[%- l('Primary for locale:') -%]</th>
    <td>[%- edit.display_data.primary_for_locale ? l('Yes') : l('No') -%]</td>
  </tr>
  <tr>
    <th>[%- l('Type:') -%]</th>
    <td>[%- edit.display_data.type.name | html -%]</td>
  </tr>
  <tr>
    <th>[%- l('Begin date:') -%]</th>
    <td>[%- edit.display_data.begin_date.format | html -%]</td>
  </tr>
  <tr>
    <th>[%- l('End date:') -%]</th>
    <td>[%- edit.display_data.end_date.format | html -%]</td>
  </tr>
</table>
