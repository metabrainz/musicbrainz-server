<table class="details add-release-label">
    [% UNLESS edit.preview %]
    <tr>
        <th>[% l('Release:') %]</th>
        <td>[% descriptive_link(edit.display_data.release) %]</td>
    </tr>
    [% END %]
    <tr>
        <th>[% l('Label:') %]</th>
        <td>[% link_entity(edit.display_data.label) IF edit.display_data.label %]</td>
    </tr>
    <tr>
        <th>[% l('Catalog number:') %]</th>
        <td>[% edit.display_data.catalog_number %]</td>
    </tr>
</table>
