[%~ WRAPPER "layout.tt" title=title ? event.name _ " - ${title}" : event.name ~%]
    <div id="content">
        [%~ React.embed(c, 'event/EventHeader', { event => event, page => page }) ~%]
        [%~ content ~%]
    </div>

    [%~ IF !full_width ~%]
        [%~ React.embed(c, 'layout/components/sidebar/EventSidebar', {event => event}) ~%]
    [%~ END ~%]
[%~ END ~%]
