{
  "ignore": [
    "node_modules",
    "root/static/lib",
    "root/static/scripts/tests/typeInfo.js"
  ],
  "presets": [
    ["env", {
      "targets": {
        "chrome": "49",
        "edge": "14",
        "firefox": "52",
        "ie": "11",
        "node": "6",
        "safari": "9.0"
      }
    }],
    "stage-2",
    "flow"
  ],
  "plugins": [
    "transform-react-jsx",
    "transform-react-constant-elements",
    "transform-runtime"
  ]
}
